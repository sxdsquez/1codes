f = open ('17.txt') #считываем файл

s = list(map(int, f.readlines())) #заносим числа из файла в список

k = 0 #кол-во кураторов-бедолаг

for i in range (len(s)): #проходимся по спсику

    if s[i] % 2 != 0 or s[i] <= 1000: #проверяем, что число нечетное или не превышает 1 000

        k += 1 #увеличиваем кол-во курторов-бедолаг

print(k) 

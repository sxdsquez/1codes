f = open ('17_2.txt') #считываем файл

s = list(map(int, f.readlines())) #создаем список

k = 0 #кол-во домов, которые обошел ютубер

maxnum = -10**10 #максимальный номер дома, в котором был Леша

for i in range (len(s)): #проходим по списку
    
    if s[i] % 2 == 0 and int(str(s[i])[-2]) % 2 != 0 and s[i] > 99 and s[i] < 1000: #проверяем, что номер дома соответствует условию
        
        k += 1 #увеличиваем кол-во домов

        maxnum = max(maxnum,s[i]) #запоминаем макс. номер дома

print(k, maxnum)
